<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:local="clr-namespace:ZavodHelper">
    <DataTemplate DataType="{x:Type local:InfoViewModel}">
        <local:InfoPage/>
    </DataTemplate>
    <DataTemplate DataType="{x:Type local:AddViewModel}">
        <local:AddPage/>
    </DataTemplate>
    <DataTemplate DataType="{x:Type local:EditViewModel}">
        <local:EditPage/>
    </DataTemplate>
    <DataTemplate DataType="{x:Type local:MapViewModel}">
        <local:Map/>
    </DataTemplate>
</ResourceDictionary>